<h1>Books#edit</h1>

<%= form_with model:@book, url:book_path(@book.id), method: :patch, do |f| %>
  <div class="new-book">
      <p>Title</p>
      <%= f.text_field:title %>
      <p>Body</p>
      <%=f.text_area :body %>
    </div>

    <div class="new-book">
      <%=f.submit'Update Book'%>
    </div>
  <%end%>
